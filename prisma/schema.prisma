generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Usuario {
  id       Int     @id @default(autoincrement())
  email    String  @unique
  password String
}

model Boleto {
  id        Int    @id @default(autoincrement())
  nombre    String
  precio    Float
}